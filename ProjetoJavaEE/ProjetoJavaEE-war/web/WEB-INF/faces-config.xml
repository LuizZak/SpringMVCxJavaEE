<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>inicio</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>telaCadastrarVeiculo</from-outcome>
            <to-view-id>/veiculo/novoVeiculo.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>telaListarVeiculos</from-outcome>
            <to-view-id>/veiculo/listar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>telaVeiculoPrincipal</from-outcome>
            <to-view-id>/veiculo/principal.xhtml</to-view-id>
        </navigation-case>
   </navigation-rule>
</faces-config>